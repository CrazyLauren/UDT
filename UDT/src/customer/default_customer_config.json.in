{

/** The customer have plugins architecture.
This option is set up options of loaded
plugins.
*/
        "modules":
		{
			/** Where the customer is looking for plugins*/
			
			"modules_path":"@_CONFIG_PLUGIN_DIR@",
			
			/** if true the customer loaded only specialized in key "libraries"
				library.
			*/
			"only_specified":"true",
			
			/** Here setting up option of loaded library.
				The key is euqal name of library (without lib, .so and .dll prefix)
			*/
			"libraries":
			{
				/** The library name
				*/
				@_LIBRARIES@
			}
        },

	/** The name of module for connection to
	kernel.
	*/
		"worker":"tcp_client_io_manager",

}