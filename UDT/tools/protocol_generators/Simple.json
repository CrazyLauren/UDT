{
  "author": "https://github.com/CrazyLauren",
  "name": "EasyProtocol",
  "description": "Simple process model in JSON format",
  "organization": "Free Software",
  "version": {
    "major": 1,
    "minor": 0,
    "revision": 52
  },
  "license": "",
  "integerOptions": [{
    "name": "n",
    "description": "number of stages",
    "min": 3,
    "max": 999,
    "value": 4
  }],
  "header":
  {
    "name": "msg_head",
    "description": "A message header",
    "full_size": "1",
    "fields":
    [
      {
        "type": "uint32_t",
        "bits": 8,
        "id": "number",
        "description": "Message number"
      },
      {
        "type": "uint32_t",
        "bits": 16,
        "id": "size",
        "description": "Message size"
      },
      {
        "type": "uint32_t",
        "bits": 4,
        "id": "version_minor",
        "description": "Message minor version"
      },
      {
        "type": "uint32_t",
        "bits": 4,
        "id": "version_major",
        "description": "Message major version"
      }
    ]
  },
  "messages": [ {
    "number": 1,
    "name": "sub_sub_msg",
    "description": "The Grand child message",
    "full_size": "1",
    "fields":
    [
      {
        "type": "char",
        "array_len": 12,
        "id": "sub_msg",
        "description": "message of Grand child"
      }
    ]
  },{
    "number": 0,
    "name": "sub_msg",
    "description": "The child message",
    "full_size": "12",
    "parent": "1",
    "fields":
    [
      {
        "type": "char",
        "array_len": 8,
        "id": "child_msg",
        "description": "message of child child"
      }
    ]
  },{
    "number": 2,
    "name": "parent_msg",
    "description": "The parent message",
    "full_size": "12",
    "parent": "0",
    "fields":
    [
      {
        "type": "char",
        "array_len": 8,
        "id": "parent_msg",
        "description": "message of parent child"
      }
    ]
  }
  ]
}
